B,SAA7B,EAAoC;AAAA;AAApC,MAAoC,WAApC,CAEE,EADF,WACE,CAXN,CAyBE,EAbF,mBAAA,CAEM,KAFN,EAEM;AAFA,MAAA,GAAG,EAAA,MAAM,CAET;AAFa,MAAA,EAAE,EAAA,MAAM,CAErB;AAFwB,MAAA,KAAK,EAAC;AAE9B,KAFN,E,CACI,mBAAA,CAAkE,MAAlE,EAAkE;AAA5D,MAAA,KAAK,EAAC,iBAAsD;AAAnC,MAAA,GAAG,EAAA,QAAQ,CAAwB;AAApB,MAAA,EAAE,EAAA,QAAQ;AAAU,KAAlE,E,IAAA,E;;AAAA,M,WAAA,C,CADJ,E;;AAAA,M,WAAA,CAaE,EAVF,WAUE,CA1BN,C,EA2BA,mBAAA,CAEM,KAFN,EAEM,IAFN,EAEM,CADF,mBAAA,CAAoF,UAApF,EAAoF;AAAzE,MAAA,GAAG,EAAA,MAAM,CAAgE;AAA5D,MAAA,EAAE,EAAA,MAAM,CAAoD;AAAjD,MAAA,WAAW,EAAC,gBAAqC;AAApB,MAAA,QAAQ,EAAR;AAAoB,KAApF,E,IAAA,E;;AAAA,M,WAAA,CACE,CAFN,C,CA5BJ,C;GA+BM,CA/BN,E;;AAAA,GAgCE,EAjCN,CAkCG,CAxDP,CAyDE,CA1DN,C,EA2DA,W","sourcesContent":["<template>\r\n    <div class=\"main-content\">\r\n        <main>\r\n            <div class=\"page-header\">\r\n                <div class=\"header-input\">\r\n                    <h1>\r\n                        Test Dashboard\r\n                    </h1>\r\n                    <small>\r\n                        Monitor key metrics:\r\n                    <input v-on:keyup.enter=\"execute\" id=\"metrics\" maxlength=\"12\" :placeholder=\"id_\" v-model=\"v\">\r\n                    </small>\r\n                </div>\r\n                <div class=\"header-actions\">\r\n                    <button>\r\n                        <span class=\"las la-file-export\"></span>\r\n                        Export\r\n                    </button>\r\n                    <button>\r\n                        <span class=\"las la-tools\"></span>\r\n                        Settings\r\n                    </button>\r\n                </div>\r\n            </div>\r\n            <div class=\"cards\">\r\n                <div v-for=\"i in c_nums\" :key=\"i\" class=\"card-single\">\r\n                    <div class=\"card-flex\">\r\n                        <div class=\"card-info\">\r\n                            <div class=\"card-head\">\r\n                                <span :ref=\"'c'+i\" :id=\"'c'+i\">{{mac}}</span>\r\n                                <span :ref=\"'n'+i\" :id=\"'n'+i\">{{sn}}</span>\r\n                            </div>\r\n                            <div>\r\n                            <small>Next:</small>\r\n                            <small :ref=\"'nf'+i\" :id=\"'nf'+i\">None</small>\r\n                            </div>\r\n                            <h2 :ref=\"'p'+i\" :id=\"'p'+i\">Waiting</h2>\r\n                            <small>PASS: 123, FAIL: 10</small>\r\n                        </div>\r\n                        <div :ref=\"'q'+i\" :id=\"'q'+i\" class=\"card-chart success\">\r\n                            <span class=\"las la-fire-alt\" :ref=\"'run'+i\" :id=\"'run'+i\"></span>\r\n                        </div>\r\n                        <div class=\"running\">\r\n                            <div class=\"outer\">\r\n                                <div class=\"body\">\r\n                                    <div class=\"arm behind\"></div>\r\n                                    <div class=\"arm front\"></div>\r\n                                    <div class=\"leg behind\"></div>\r\n                                    <div class=\"leg front\"></div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div>\r\n                        <textarea :ref=\"'t'+i\" :id=\"'t'+i\" placeholder=\"Log Message...\" readonly></textarea>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </main>\r\n    </div>\r\n    <label for=\"sidebar-toggle\" class=\"body-label\"></label>\r\n</template>\r\n\r\n<script>\r\n/* eslint no-eval: 0 */\r\nimport axios from 'axios'\r\nimport qs from 'qs'\r\n\r\nvar r1 = 0; var r2 = 0; var r3 = 0; var r4 = 0; var r5 = 0; var r6 = 0\r\n\r\nfunction rman (r) {\r\n  var a\r\n  const id = 'q' + r\r\n  const chart = 'run' + r\r\n  a = document.getElementById(id)\r\n  // a.className = \"las\"\r\n  // a.innerHTML = \"&#xf7c5;\";\r\n  setTimeout(function () {\r\n    a.innerHTML = \"<span class='las la-fire-alt' id='\" + chart + \"' style='font-size:5rem;color:seagreen'> </span>\"\r\n    // a.innerHTML = '&#xf7c9'\r\n  }, 1000)\r\n  setTimeout(function () {\r\n    a.innerHTML = \"<span class='las la-burn' id='\" + chart + \"' style='font-size:5rem;color:seagreen'> </span>\"\r\n    // a.innerHTML = '&#xf7ce;'\r\n  }, 2000)\r\n  setTimeout(function () {\r\n    a.innerHTML = \"<span class='las la-fire' id='\" + chart + \"' style='font-size:5rem;color:seagreen'> </span>\"\r\n    // a.innerHTML = '&#xf7c5;'\r\n  }, 3000)\r\n}\r\n\r\nfunction runCard (s, v) {\r\n  if (s === 'run') {\r\n    if (v === 'c1' && !(r1)) {\r\n      r1 = setInterval(function () { rman('1') }, 3000)\r\n    } else if (v === 'c2' && !(r2)) {\r\n      r2 = setInterval(function () { rman('2') }, 3000)\r\n    } else if (v === 'c3' && !(r3)) {\r\n      r3 = setInterval(function () { rman('3') }, 3000)\r\n    } else if (v === 'c4' && !(r4)) {\r\n      r4 = setInterval(function () { rman('4') }, 3000)\r\n    } else if (v === 'c5' && !(r5)) {\r\n      r5 = setInterval(function () { rman('5') }, 3000)\r\n    } else if (v === 'c6' && !(r6)) {\r\n      r6 = setInterval(function () { rman('6') }, 3000)\r\n    }\r\n  } else {\r\n    console.log('stop', v)\r\n    if (v === 'c1') {\r\n      clearInterval(r1)\r\n      r1 = 0\r\n    } else if (v === 'c2') {\r\n      clearInterval(r2)\r\n      r2 = 0\r\n    } else if (v === 'c3') {\r\n      clearInterval(r3)\r\n      r3 = 0\r\n    } else if (v === 'c4') {\r\n      clearInterval(r4)\r\n      r4 = 0\r\n    } else if (v === 'c5') {\r\n      clearInterval(r5)\r\n      r5 = 0\r\n    } else if (v === 'c6') {\r\n      clearInterval(r6)\r\n      r6 = 0\r\n    }\r\n  }\r\n}\r\n\r\nfunction run (c, s) {\r\n  axios({\r\n    method: 'post',\r\n    url: 'http://127.0.0.1:8000/api/data/',\r\n    data: qs.stringify({\r\n      card: c,\r\n      status: s\r\n    }),\r\n    headers: {\r\n      'content-type': 'application/x-www-form-urlencoded;charset=utf-8'\r\n    }\r\n  })\r\n    .then(function (response) {\r\n      const e = response.data\r\n      var num = e.card[1]\r\n      const log = document.getElementById('t' + num)\r\n      log.value = e.log\r\n      const p = document.getElementById('p' + num)\r\n      const nf = document.getElementById('nf' + num)\r\n      if (p.textContent !== e.flow) {\r\n        p.textContent = e.flow\r\n        nf.textContent = e.nflow\r\n      }\r\n      if (log.value.length !== e.log.length) {\r\n        log.scrollTo(0, log.scrollHeight)\r\n      }\r\n      if (e.status === 'running') {\r\n        runCard('run', e.card)\r\n        run(e.card, e.status)\r\n      } else if (e.status === 'pass') {\r\n        runCard('stop', e.card)\r\n        c = document.getElementById(e.card)\r\n        c.style.color = 'seagreen'\r\n        c = document.getElementById('n' + num)\r\n        c.style.color = 'seagreen'\r\n      } else if (e.status === 'fail') {\r\n        runCard('stop', e.card)\r\n        c = document.getElementById(e.card)\r\n        c.style.color = '#d96c4e'\r\n        c = document.getElementById('n' + num)\r\n        c.style.color = '#d96c4e'\r\n        console.log('fail')\r\n        console.log(e)\r\n      }\r\n    })\r\n    .catch((error) => console.log(error))\r\n}\r\n\r\nexport const eval2 = fn => {\r\n  var Fun = Function\r\n  return new Fun('return ' + fn)()\r\n}\r\n\r\nexport default {\r\n  name: 'metrics',\r\n  data () {\r\n    return {\r\n      // metrics parameter\r\n      id_: 'ID',\r\n      c_nums: 8,\r\n      v: '',\r\n      runid: '0',\r\n      // mac, sn\r\n      mac: '************',\r\n      sn: '************'\r\n    }\r\n  },\r\n  mounted () {\r\n    for (let i = 1; i <= this.c_nums; i++) {\r\n      axios({\r\n        method: 'post',\r\n        url: 'http://127.0.0.1:8000/api/data/',\r\n        data: qs.stringify({\r\n          card: 'c' + i.toString(),\r\n          status: 'restore'\r\n        }),\r\n        headers: {\r\n          'content-type': 'application/x-www-form-urlencoded;charset=utf-8'\r\n        }\r\n      })\r\n        .then(function (response) {\r\n          run('c' + i.toString(), 'restore')\r\n          console.log(response)\r\n        })\r\n        .catch((error) => console.log(error))\r\n    }\r\n  },\r\n  methods: {\r\n    execute (event) {\r\n      if (this.id_ === 'ID' && !(eval('r' + this.v).valueOf())) {\r\n        this.runid = this.v\r\n        this.id_ = 'MAC'\r\n        this.v = ''\r\n        this.$refs['c' + this.runid].textContent = this.mac\r\n        this.$refs['n' + this.runid].textContent = this.sn\r\n        this.$refs['c' + this.runid].style.color = '#0120cc'\r\n        this.$refs['n' + this.runid].style.color = '#0120cc'\r\n      } else if (this.id_ === 'MAC') {\r\n        this.id_ = 'SN'\r\n        this.$refs['c' + this.runid].textContent = this.v\r\n        this.v = ''\r\n      } else if (this.id_ === 'SN') {\r\n        this.id_ = 'ID'\r\n        this.$refs['n' + this.runid].textContent = this.v\r\n        // this.$refs['c' + this.runid].style.color = '#555'\r\n        // this.$refs['n' + this.runid].style.color = '#555'\r\n        this.v = ''\r\n        run('c' + this.runid, 'start')\r\n        // runCard('run', this.runid)\r\n      } else {\r\n        alert('input error')\r\n        this.v = ''\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n#sidebar-toggle {\r\n    display: none;\r\n}\r\n\r\n.body-label {\r\n    position: fixed;\r\n    height: 100%;\r\n    width: calc(100% - 250px);\r\n    z-index: 30;\r\n    right: -100%;\r\n    top: 0;\r\n}\r\n\r\n#sidebar-toggle:checked ~ .sidebar {\r\n    left: -100%;\r\n}\r\n\r\n#sidebar-toggle:checked ~ header {\r\n    left: 0;\r\n    width: 100%;\r\n    right: 0;\r\n}\r\n\r\n#sidebar-toggle:checked ~ .main-content {\r\n    margin-left: 0;\r\n}\r\n\r\n.main-content {\r\n    margin-left: 250px;\r\n    transition: margin-left 300ms;\r\n}\r\n\r\nmain {\r\n    padding: 1.5rem;\r\n    /*background: #f1f5f9;*/\r\n    min-height: calc(100vh - 70px);\r\n    margin-top: 70px;\r\n    height: 100%;\r\n    /* max-height: 600px; */\r\n    /*width: 1000px;*/\r\n    background-color: hsla(100,40%,30%,.2);\r\n    background-image:\r\n        url('../../image/tumblr_p7n8kqHMuD1uy4lhuo1_540.png'),\r\n        url('../../image/tumblr_p7n908E1Jb1uy4lhuo1_1280.png'),\r\n        url('../../image/tumblr_p7n8kqHMuD1uy4lhuo2_1280.png'),\r\n        url('../../image/tumblr_p7n8on19cV1uy4lhuo1_1280.png'),\r\n        url('../../image/tumblr_p7n8kqHMuD1uy4lhuo3_1280.png');\r\n    background-repeat: repeat-x;\r\n    background-position:\r\n        0 30%,\r\n        0 100%,\r\n        0 50%,\r\n        0 100%,\r\n        0 0;\r\n    background-size:\r\n        2500px,\r\n        800px,\r\n        500px 200px,\r\n        1000px,\r\n        400px 260px;\r\n    animation: 100s para infinite linear;\r\n}\r\n\r\n@keyframes para {\r\n    100% {\r\n        background-position:\r\n        -5000px 20%,\r\n        -800px 95%,\r\n        500px 50%,\r\n        1000px 100%,\r\n        400px 0;\r\n    }\r\n}\r\n\r\n.metrics {\r\n    border-top: none;\r\n    border-left: none;\r\n    border-right: none;\r\n    border-bottom-width: 3px;\r\n    text-align:center;\r\n    outline: none;\r\n    background-color: transparent;\r\n}\r\n\r\n.header-input input {\r\n    border: none;\r\n    border-bottom: 3px solid #404040;\r\n    font-size: 1.2rem;\r\n    background-color: transparent;\r\n    text-align:center;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.header-input input:focus {\r\n    outline:none;\r\n}\r\n\r\n.page-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\n.header-actions button {\r\n    outline: none;\r\n    color: #fff;\r\n    background: #4cad89;\r\n    border: none;\r\n    padding: .6rem 1rem;\r\n    margin-left: 1rem;\r\n    border-radius: 3px;\r\n    font-weight: 600;\r\n}\r\n\r\n.header-actions button span {\r\n    font-size: 1.2rem;\r\n    margin-right: .6rem;\r\n}\r\n\r\n.cards {\r\n    display: grid;\r\n    grid-template-columns: repeat(4, 1fr);\r\n    min-height: calc(40vh - 70px);\r\n    grid-gap: 3rem;\r\n    margin-top: 2rem;\r\n}\r\n\r\n.cards textarea {\r\n    padding: .1rem .4rem;\r\n    background:#39395200;\r\n    border-radius: 5px;\r\n    width: 100%;\r\n    height: calc(25vh - 70px);\r\n    white-space: pre;\r\n    overflow-wrap: normal;\r\n    overflow-x: auto;\r\n    font-size: 1rem;\r\n    color: #0120cc;\r\n    resize:none;\r\n    outline: none;\r\n    border: none;\r\n}\r\n\r\n.cards textarea::placeholder {\r\n    position: relative;\r\n    color: rgb(66 107 46 / 28%);\r\n    font-size: 1rem;\r\n    text-align: center;\r\n    top:50%;\r\n    transform:translateY(-50%);\r\n}\r\n\r\n.cards textarea::-webkit-scrollbar {\r\n    cursor: auto;\r\n    width: 7px;\r\n    height: 7px;\r\n    background-color: whitesmoke;\r\n}\r\n\r\n.cards textarea::-webkit-scrollbar-track {\r\n    -webkit-box-shadow: inset 0 0 6px rgb(0 0 0 / 30%);\r\n    border-radius: 10px;\r\n    background-color: whitesmoke;\r\n}\r\n\r\n.cards textarea::-webkit-scrollbar-thumb {\r\n    border-radius: 10px;\r\n    -webkit-box-shadow: inset 0 0 6px rgb(0 0 0 / 30%);\r\n    background-color: #555555;\r\n}\r\n\r\n.card textarea:focus {\r\n    outline: none;\r\n    border:dotted 2px;\r\n}\r\n\r\n.card-single {\r\n    border-radius: 5px;\r\n    background: #eeeeeed1;\r\n    /*background: #fff;*/\r\n    padding: .5rem;\r\n    box-shadow: 4px 4px 10px rgba(0,0,0,0.1);\r\n    transform:scale(1,1);\r\n    transition: all 0.5s ease-out;\r\n}\r\n\r\n.card-single:hover {\r\n    transform:scale(1.06,1.05);\r\n    box-shadow: 4px 4px 10px rgba(0,0,0,0.5);\r\n}\r\n\r\n.card-flex {\r\n    display: grid;\r\n    grid-template-columns: 75% auto;\r\n    padding: .1rem .4rem;\r\n    padding-bottom: .3rem;\r\n    align-items: cente